{
    "name": "grantholle/pear-openid",
    "description": "A psr-4 port of the Pear package: PHP implementation of OpenID 1.1 and 2.0",
    "license": "BSD-2-Clause",
    "support": {
        "issues": "https://github.com/grantholle/pear-openid/issues",
        "source": "https://github.com/grantholle/pear-openid"
    },
    "type": "library",
    "authors": [
        {
            "email": "shupp@php.net",
            "name": "Bill Shupp",
            "role": "Lead"
        },
        {
            "email": "cweiske@php.net",
            "name": "Christian Weiske",
            "role": "Developer"
        },
        {
            "name": "Grant Holle",
            "homepage": "https://grantholle.dev"
        }
    ],
    "autoload": {
        "psr-4": {
            "Pear\\OpenId\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "require": {
        "php": "^7.2",
        "grantholle/pear-crypt-diffiehellman": "^1.0",
        "grantholle/pear-net-url2": "^3.0",
        "grantholle/pear-services-yadis": "^1.0",
        "grantholle/pear-http-request2": "^3.0",
        "grantholle/pear-cache-lite": "^2.0",
        "ext-mbstring": "*",
        "ext-libxml": "*",
        "ext-dom": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "ext-simplexml": "*"
    }
}
